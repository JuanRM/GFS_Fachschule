<h1>New Administrator</h1>
</br>

<%= form_for @user do |f| %>

  <div class="field">
    <%= f.label :email %><br />
    <%= f.text_field :email %>
  </div>
  </br>

  <div class="field">
    <%= f.label :password %><br />
    <%= f.password_field :password %>
  </div>
  </br>


  <div class="field">
    <%= f.label :password_confimation %><br />
    <%= f.password_field :password_confirmation %>
  </div>
  </br>


  <!--div class="field">
   < f.label :password_confirmation %><br />
   < f.password_field :password_confirmation %>
  </div-->

  <div class="actions">
    <%= submit_tag "Create Admin" %>
  </div>
  

<% end %>


